{
  "name": "Muisc",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "prisma:push": "npx prisma db push --schema=apps/api/prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=apps/api/prisma/schema.prisma",
    "prisma:studio": "npx prisma studio --schema=apps/api/prisma/schema.prisma",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types"
